<launch>
  
    <node pkg="svo_ros" type="vo" name="svo" clear_params="true" output="screen">
    
        <!-- Camera topic to subscribe to -->
        <param name="cam_topic" value="/bebop/image_raw" type="str" />
        
        <!-- Camera calibration file -->
        <rosparam file="$(find svo_ros)/param/camera_pinhole.yaml" /> <!-- "$(find svo_ros)/param/camera_atan.yaml" | "$(find svo_ros)/param/camera_pinhole.yaml" -->
        
        <!-- Default parameter settings: choose between vo_fast and vo_accurate -->
        <rosparam file="$(find svo_ros)/param/vo_accurate.yaml" />

        <!-- Initial camera orientation, make it point downwards 3.05    1.57-->
	    <param name="init_rx" value="-3.05" /> 
	    <param name="init_ry" value="0.00" />
	    <param name="init_rz" value="0.00" /> 

        <param name="init_tx" value="0.00" />
        <param name="init_ty" value="0.00" />
        <param name="init_tz" value="11.00" /> <!-- 20.00 m = 1.00 svo-->

    </node>


    <!-- RVIZ + SVO GUI -->
    <!-- <node name="vis" pkg="rviz" type="rviz" args=" -d $(find svo_ros)/rviz_config.rviz" /> -->
        
</launch>
